  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "knex": "^0.13.0",
    "pg": "^7.1.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "flow-bin": "^0.53.1",
    "flow-typed": "^2.1.5",
    "nodemon": "^1.11.0"
  }
